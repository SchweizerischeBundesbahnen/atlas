{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "didok-frontend": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/didok-frontend",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": ["src/favicons", "src/assets"],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "./node_modules/@arcgis/core/assets/esri/themes/light/main.css",
              "src/styles.scss"
            ],
            "scripts": [],
            "allowedCommonJsDependencies": ["moment", "uuid"]
          },
          "configurations": {
            "production": {
              "outputPath": "dist",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ]
            },
            "es5": {
              "tsConfig": "tsconfig.app.es5.json"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "didok-frontend:build",
            "proxyConfig": "proxy.conf.test.js"
          },
          "configurations": {
            "production": {
              "browserTarget": "didok-frontend:build:production"
            },
            "es5": {
              "browserTarget": "didok-frontend:build:es5"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": ["src/favicons", "src/assets"],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "ci": {
              "browsers": "ChromeHeadless",
              "codeCoverage": true,
              "sourceMap": false,
              "progress": false,
              "watch": false,
              "reporters": ["dots", "sonarqube"]
            }
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["src/**/*.ts", "src/**/*.html"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "didok-frontend:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "didok-frontend:serve:production"
            },
            "puppeteer": {
              "protractorConfig": "e2e/protractor.puppeteer.conf.js"
            },
            "browserstack": {
              "protractorConfig": "e2e/protractor.browserstack.conf.js"
            }
          }
        },
        "ngsscbuild": {
          "builder": "angular-server-side-configuration:ngsscbuild",
          "options": {
            "additionalEnvironmentVariables": [],
            "browserTarget": "didok-frontend:build",
            "ngsscEnvironmentFile": "src/environments/environment.prod.ts",
            "filePattern": "**/index.html"
          },
          "configurations": {
            "production": {
              "browserTarget": "didok-frontend:build:production"
            }
          }
        },
        "t9n": {
          "builder": "angular-t9n:t9n",
          "options": {
            "translationFile": "src/i18n/messages.xlf",
            "targetTranslationPath": "src/i18n",
            "includeContextInTarget": false
          }
        }
      }
    }
  },
  "defaultProject": "didok-frontend",
  "cli": {
    "analytics": false
  }
}
