{
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "docker": "sh ./prepare-docker.sh",
    "publish-frontend": "sh ./npm_publish.sh",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --no-watch --browsers=ChromeHeadlessNoSandbox --code-coverage",
    "test-debug": "ng test --watch true --browsers=Chrome",
    "lint": "ng lint",
    "prepare": "cd .. && husky install frontend/.husky",
    "format": "prettier --write src/**/*.ts",
    "build:ngssc": "ng run atlas-frontend:ngsscbuild:production",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:open-e2e-release": "cypress open --config specPattern=cypress/release",
    "cypress:run-e2e-release": "cypress run --config specPattern=cypress/release",
    "generate-api": "openapi-generator-cli generate -g typescript-angular -i ../apim-configuration/src/main/resources/local/spec.yaml -o src/app/api --type-mappings=date=Date --additional-properties apiModulePrefix=Atlas"
  },
  "name": "atlas-frontend",
  "repository": {
    "type": "git",
    "url": "git+https://code.sbb.ch/scm/ki_atlas/atlas-frontend.git"
  },
  "version": "1.356.0",
  "files": [
    "dist/*"
  ],
  "prettier": {
    "singleQuote": true,
    "printWidth": 100
  },
  "lint-staged": {
    "frontend/src/**/*": "frontend/node_modules/prettier/bin-prettier.js --write --ignore-unknown"
  },
  "dependencies": {
    "@angular/animations": "16.1.8",
    "@angular/cdk": "16.1.7",
    "@angular/common": "16.1.8",
    "@angular/compiler": "16.1.8",
    "@angular/core": "16.1.8",
    "@angular/forms": "16.1.8",
    "@angular/material": "16.1.7",
    "@angular/material-moment-adapter": "16.1.7",
    "@angular/platform-browser": "16.1.8",
    "@angular/platform-browser-dynamic": "16.1.8",
    "@angular/router": "16.1.8",
    "@angular/service-worker": "16.1.8",
    "@ng-select/ng-select": "11.0",
    "@ngx-translate/core": "^15.0",
    "@ngx-translate/http-loader": "^8.0",
    "angular-oauth2-oidc": "^15.0",
    "angular-server-side-configuration": "^16.0",
    "bootstrap": "^5.3",
    "jwt-decode": "^3.1",
    "maplibre-gl": "^3.1",
    "moment": "^2.29",
    "ngx-color-picker": "^14.0",
    "proj4": "^2.9.0",
    "rxjs": "^7.8",
    "tslib": "^2.6",
    "zone.js": "^0.13"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.x",
    "@angular-devkit/build-angular": "16.1.8",
    "@angular-eslint/builder": "^16.0",
    "@angular-eslint/eslint-plugin": "^16.0",
    "@angular-eslint/eslint-plugin-template": "^16.0",
    "@angular-eslint/schematics": "^16.0",
    "@angular-eslint/template-parser": "^16.0",
    "@angular/cli": "16.1.8",
    "@angular/localize": "16.1.8",
    "@cypress/schematic": "^2.5",
    "@openapitools/openapi-generator-cli": "~2.6",
    "@playwright/test": "^1.35",
    "@types/jasmine": "^4.3",
    "@types/node": "^20.3",
    "@types/proj4": "^2.5.2",
    "@typescript-eslint/eslint-plugin": "^5.60",
    "@typescript-eslint/parser": "^5.60",
    "cypress": "^12.16",
    "cypress-failed-log": "^2.10",
    "cypress-high-resolution": "^1.0",
    "eslint": "^8.43",
    "eslint-config-prettier": "^8.8",
    "husky": "^8.0",
    "jasmine-core": "^5.0",
    "karma": "^6.4",
    "karma-browserstack-launcher": "^1.6",
    "karma-chrome-launcher": "^3.2",
    "karma-coverage": "^2.2",
    "karma-jasmine": "^5.1",
    "karma-jasmine-html-reporter": "^2.1",
    "karma-sonarqube-reporter": "^1.4",
    "lint-staged": "^13.2",
    "prettier": "^2.8",
    "typescript": "^5.1"
  },
  "optionalDependencies": {
    "puppeteer": "^20.7.4"
  }
}
