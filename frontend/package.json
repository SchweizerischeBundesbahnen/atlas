{
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "docker": "sh ./prepare-docker.sh",
    "publish-frontend": "sh ./npm_publish.sh",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --no-watch --browsers=ChromeHeadlessNoSandbox --code-coverage",
    "test-debug": "ng test --watch true --browsers=Chrome",
    "lint": "ng lint",
    "format": "prettier --write src/**/*.ts",
    "build:ngssc": "ng run atlas-frontend:ngsscbuild:production",
    "build-prod": "ng run atlas-frontend:ngsscbuild:production",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:open-e2e-release": "cypress open --config specPattern=cypress/release",
    "cypress:run-e2e-release": "cypress run --config specPattern=cypress/release",
    "generate-api": "openapi-generator-cli generate -g typescript-angular -i ../apim-configuration/src/main/resources/local/spec.yaml -o src/app/api --type-mappings=date=Date --additional-properties apiModulePrefix=Atlas --global-property skipFormModel=false"
  },
  "name": "atlas-frontend",
  "repository": {
    "type": "git",
    "url": "git+https://code.sbb.ch/scm/ki_atlas/atlas.git"
  },
  "version": "2.495.0",
  "files": [
    "dist/*"
  ],
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "endOfLine": "crlf"
  },
  "lint-staged": {
    "frontend/src/**/*": "frontend/node_modules/prettier/bin/prettier.cjs --write --ignore-unknown"
  },
  "dependencies": {
    "@angular/animations": "18.2.13",
    "@angular/cdk": "18.2.14",
    "@angular/common": "18.2.13",
    "@angular/compiler": "18.2.13",
    "@angular/core": "18.2.13",
    "@angular/forms": "18.2.13",
    "@angular/material": "18.2.14",
    "@angular/material-moment-adapter": "18.2.14",
    "@angular/platform-browser": "18.2.13",
    "@angular/platform-browser-dynamic": "18.2.13",
    "@angular/router": "18.2.13",
    "@angular/service-worker": "18.2.13",
    "@ng-select/ng-select": "13.9",
    "@ngx-translate/core": "^16.0.0",
    "@ngx-translate/http-loader": "^16.0.0",
    "angular-oauth2-oidc": "^17.0.2",
    "angular-server-side-configuration": "^18.2.0",
    "bootstrap": "^5.3",
    "jwt-decode": "^4.0.0",
    "maplibre-gl": "^4.7.1",
    "moment": "^2.30",
    "ngx-color-picker": "^17.0.0",
    "ngx-cookie-service": "^18.0.0",
    "proj4": "^2.12.1",
    "rxjs": "^7.8",
    "tslib": "^2.8",
    "zone.js": "^0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1902.0",
    "@angular-devkit/build-angular": "18.2.14",
    "@angular-eslint/builder": "^18.4.0",
    "@angular-eslint/eslint-plugin": "^18.4.0",
    "@angular-eslint/eslint-plugin-template": "^18.4.0",
    "@angular-eslint/schematics": "^18.4.0",
    "@angular-eslint/template-parser": "^18.4.0",
    "@angular/cli": "18.2.14",
    "@angular/localize": "18.2.13",
    "@cypress/schematic": "^2.5.2",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.13.0",
    "@openapitools/openapi-generator-cli": "~2.17.0",
    "@playwright/test": "^1.48.1",
    "@types/jasmine": "^5.1.4",
    "@types/node": "^22.7.9",
    "@types/proj4": "^2.5.5",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "cypress": "^13.15.0",
    "cypress-failed-log": "^2.10",
    "cypress-high-resolution": "^1.0",
    "cypress-mochawesome-reporter": "^3.8.2",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "jasmine-core": "^5.4",
    "karma": "^6.4",
    "karma-browserstack-launcher": "^1.6",
    "karma-chrome-launcher": "^3.2",
    "karma-coverage": "^2.2",
    "karma-jasmine": "^5.1",
    "karma-jasmine-html-reporter": "^2.1",
    "karma-sonarqube-reporter": "^1.4",
    "lint-staged": "^15.2.10",
    "prettier": "^3.5.3",
    "typescript": "^5.5"
  },
  "optionalDependencies": {
    "puppeteer": "^23.6.0"
  }
}
