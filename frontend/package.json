{
  "scripts": {
    "postinstall": "ngcc",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "docker": "sh ./prepare-docker.sh",
    "publish-frontend": "sh ./npm_publish.sh",
    "watch": "ng build --watch --configuration development",
    "test": "ng test -c ci",
    "lint": "ng lint",
    "prepare": "cd .. && husky install frontend/.husky",
    "format": "prettier --write src/**/*.ts",
    "build:ngssc": "ng run atlas-frontend:ngsscbuild:production",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run-e2e-line": "cypress run --spec cypress/integration/lidi/line/**/*.ts",
    "cypress:run-e2e-subline": "cypress run --spec cypress/integration/lidi/subline/**/*.ts",
    "cypress:run-e2e-ttfn": "cypress run --spec cypress/integration/ttfn/**/*.ts",
    "cypress:run-e2e-release": "cypress run --config integrationFolder=cypress/release",
    "cypress:open-e2e-release": "cypress open --config integrationFolder=cypress/release",
    "generate-api": "openapi-generator-cli generate -g typescript-angular -i ../apim-configuration/src/main/resources/api-dev/spec.yaml -o src/app/api --type-mappings=date=Date --additional-properties apiModulePrefix=Atlas"
  },
  "name": "atlas-frontend",
  "repository": {
    "type": "git",
    "url": "git+https://code.sbb.ch/scm/ki_atlas/atlas-frontend.git"
  },
  "version": "0.433.0",
  "files": [
    "dist/*"
  ],
  "prettier": {
    "singleQuote": true,
    "printWidth": 100
  },
  "lint-staged": {
    "frontend/src/**/*": "frontend/node_modules/prettier/bin-prettier.js --write --ignore-unknown"
  },
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/cdk": "^14.0.0",
    "@angular/common": "~14.0.0",
    "@angular/compiler": "~14.0.0",
    "@angular/core": "~14.0.0",
    "@angular/forms": "~14.0.0",
    "@angular/material": "^14.0.0",
    "@angular/material-moment-adapter": "^14.0.0",
    "@angular/platform-browser": "~14.0.0",
    "@angular/platform-browser-dynamic": "~14.0.0",
    "@angular/router": "~14.0.0",
    "@ng-select/ng-select": "^8.1.1",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "angular-oauth2-oidc": "^13.0.1",
    "angular-server-side-configuration": "^13.1.0",
    "bootstrap": "^5.1.3",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.3",
    "ngx-color-picker": "^12.0.1",
    "rxjs": "~7.5.5",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.5"
  },
  "optionalDependencies": {
    "puppeteer": "^13.5.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.3.0",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "^13.3.0",
    "@angular/compiler-cli": "~14.0.0",
    "@angular/localize": "^14.0.0",
    "@cypress/schematic": "^1.6.0",
    "@openapitools/openapi-generator-cli": "^2.4.26",
    "@types/jasmine": "~4.0.0",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "cypress": "^9.6.1",
    "cypress-failed-log": "^2.9.5",
    "cypress-high-resolution": "^1.0.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.4",
    "jasmine-core": "~4.0.1",
    "karma": "~6.3.17",
    "karma-browserstack-launcher": "^1.6.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "karma-sonarqube-reporter": "^1.4.0",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.1",
    "typescript": "~4.6.3"
  }
}